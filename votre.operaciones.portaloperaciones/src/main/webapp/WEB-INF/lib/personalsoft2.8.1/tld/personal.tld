<?xml version = '1.0' encoding = 'ISO-8859-1'?>
<!DOCTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.1//EN" "http://java.sun.com/j2ee/dtds/web-jsptaglibrary_1_1.dtd">
<taglib>
   <tlibversion>1.1</tlibversion>
   <jspversion>1.1</jspversion>
   <shortname>personal</shortname>
   <uri>personal</uri>
   <info>Custom Tags para framework.</info>
   <tag>
		<name>tienePermiso</name>
		<tagclass>co.com.personalsoft.base.tld.Autorizador</tagclass>
		<bodycontent>JSP</bodycontent>
		<attribute>
			<name>perfiles</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>claseSeguridad</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<name>noTienePermiso</name>
		<tagclass>co.com.personalsoft.base.tld.Negador</tagclass>
		<bodycontent>JSP</bodycontent>
		<attribute>
			<name>perfiles</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>claseSeguridad</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<name>mensajes</name>
		<tagclass>co.com.personalsoft.base.tld.ListaMensajes</tagclass>
		<bodycontent>JSP</bodycontent>
		<attribute>
			<name>listaMensajes</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<name>construirMenu</name>
		<tagclass>co.com.personalsoft.seguridad.tld.GeneradorMenu</tagclass>
		<bodycontent>JSP</bodycontent>
		<attribute>
			<name>menuXml</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
</taglib>